#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/wd@1.14.0/node_modules/wd/lib/node_modules:/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/wd@1.14.0/node_modules/wd/node_modules:/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/wd@1.14.0/node_modules:/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/wd@1.14.0/node_modules/wd/lib/node_modules:/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/wd@1.14.0/node_modules/wd/node_modules:/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/wd@1.14.0/node_modules:/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../wd/lib/bin.js" "$@"
else
  exec node  "$basedir/../wd/lib/bin.js" "$@"
fi
