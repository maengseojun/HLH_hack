#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/browserify@13.3.0/node_modules/browserify/bin/node_modules:/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/browserify@13.3.0/node_modules/browserify/node_modules:/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/browserify@13.3.0/node_modules:/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/browserify@13.3.0/node_modules/browserify/bin/node_modules:/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/browserify@13.3.0/node_modules/browserify/node_modules:/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/browserify@13.3.0/node_modules:/Users/kimhyeon/Desktop/PROJECTS/HLH_hack/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserify@13.3.0/node_modules/browserify/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../browserify@13.3.0/node_modules/browserify/bin/cmd.js" "$@"
fi
